<script>
    $(document).ready(function () {
        $('a.remote-modal').click(function (event) {
            event.preventDefault();
            var $this = $(this);
            var href = $this.attr('href');
            $.ajax({
                url: href,
                success: function (response) {
                    $('#remoteModalContent').html(response);
                    $('#remoteModal').modal('show');
                }
            });
        });

    });
</script>
